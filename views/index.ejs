<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/bootstrap/dist/css/bootstrap.min.css">     <!-- accesing to bootstrap-->
    <title>Document</title>
</head>
<body>
    <%- include('partials/navbar') %>
    <div class="container mt-3">
        <h1>Home Page</h1>
        <div class="row">
            <div class="col-12">
                <% products.forEach(products => { %>
                    <% if (products.isHome) { %>
                        <%- include('partials/products',products) %>       <!--for partial view and sending object-->
                        <% } %>
                <% }) %>
                
            </div>
        </div>
    </div>
</body>
</html>